@using AgileToolbox.Client.Models
@using Blazorise


<Blazorise.Button Color="Blazorise.Color.Primary" Clicked="CreateRoom">Create Room</Blazorise.Button>

@code {
    [Parameter]
    public EventCallback<string> OnEnterRoom { get; set; }

    private async Task CreateRoom()
    {        
        await OnEnterRoom.InvokeAsync(Guid.NewGuid().ToString());
    }
}
